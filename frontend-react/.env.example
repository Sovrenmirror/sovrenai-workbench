# Sovren AI Workbench Frontend Configuration
# Copy this file to .env.local and fill in your values

# API Base URL (default: http://localhost:3750)
VITE_API_BASE_URL=http://localhost:3750

# WebSocket Base URL (default: ws://localhost:3750)
VITE_WS_BASE_URL=ws://localhost:3750

# Development mode auto-login credentials (optional, dev only)
# These are only used if present and only in development mode
VITE_DEV_EMAIL=your_email@domain.com
VITE_DEV_PASSWORD=your_password
